{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-classes-per-file": ["error", 10],
    "linebreak-style": ["error", "windows"],
    "implicit-arrow-linebreak": "off",
    "comma-dangle": "off",
    "no-unused-vars": "off",
    "function-paren-newline": "off"
  }
}
